{
  "cSpell.words": [
    "inputgroup",
    "inputtext",
    "itemuid",
    "primereact",
    "printtestdrive",
    "taskuid",
    "useruid"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "eslint.enable": true,
  "prettier.experimentalTernaries": true,
  "prettier.endOfLine": "auto",
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
